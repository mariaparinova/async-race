{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["*.config.js"],
    "plugins": [
        "@typescript-eslint", "prettier"
    ],
    "root": true,
    "rules": {
        "@typescript-eslint/no-explicit-any": 2
    },
    "overrides": [
    ]
}
